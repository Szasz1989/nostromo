    "lodash.isequal": "^4.5.0",
    "lodash.set": "^4.3.2",
    "lodash.truncate": "^4.4.2",
    "minimist": "^1.2.0",
    "mithril": "^1.1.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1",
    "tree-kill": "^1.2.0",
    "ws": "^3.0.0"
